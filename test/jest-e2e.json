{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testEnvironment": "node",
  "testMatch": ["**/*.e2e-spec.ts"],
  "setupFilesAfterEnv": ["./test/setup/setup-test.ts"],
